{
  "name": "subject-selector-design",
  "author": "Osvaldo Martini",
  "version": "0.0.1",
  "license": "NHS",
  "private": true,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.13"
  },
  "module": "dist/subject-selector-design.esm.js",
  "size-limit": [
    {
      "path": "dist/subject-selector-design.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/subject-selector-design.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "tsdx build --entry src/index.tsx",
    "postbuild": "copyfiles -u 1 fonts/**/* dist/fonts",
    "copy-files": "copyfiles -u 1 \"src/components/**/*.css\" \"src/components/**/*.svg\" \"src/components/**/*.json\" dist/",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "release": "npm run build && npm run publish-ci",
    "publish-ci": "npm publish --registry https://nexus.mgmt.texasplatform.uk/repository/bcss-npm-private",
    "local-components": "npm link ../components",
    "latest-components": "npm unlink --no-save @bcss/react-components && npm install @bcss/react-components"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.4.7",
    "@storybook/addon-essentials": "6.4.7",
    "@storybook/addon-links": "6.4.7",
    "@storybook/components": "6.4.14",
    "@storybook/node-logger": "6.4.7",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.4.7",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/luxon": "2.0.7",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-lottie": "1.2.6",
    "babel-loader": "8.2.3",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "copyfiles": "2.4.1",
    "css-loader": "6.5.1",
    "eslint-config-react-app": "^7.0.0",
    "luxon": "2.1.1",
    "node-sass": "6.0.1",
    "postcss": "8.4.5",
    "react": "17.0.2",
    "react-bootstrap": "2.0.3",
    "react-dom": "17.0.2",
    "react-lottie": "1.2.3",
    "react-paginate": "^8.1.0",
    "react-scripts": "4.0.3",
    "react-unhook": "0.4.1",
    "rimraf": "3.0.2",
    "rollup-plugin-postcss": "4.0.2",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.2.6",
    "tsdx": "^0.14.1",
    "typescript": "4.1.2",
    "web-vitals": "1.0.1"
  },
  "peerDependencies": {
    "bootstrap": "5.1.3",
    "react": "17.0.2",
    "react-bootstrap": "2.0.3",
    "react-dom": "17.0.2"
  },
  "dependencies": {
    "axios": "^0.26.1"
  }
}
